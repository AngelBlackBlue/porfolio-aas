---
import { TAGS } from "@/data/tags";
import SkillItem from "./SkillItem.astro";

const SKILLS = [
    {
        title: 'Front End',
        tags: [TAGS.JS, TAGS.TYPESCRIPT, TAGS.HTML, TAGS.CSS, TAGS.REACT, TAGS.REDUX, TAGS.ZUSTAND, TAGS.TAILWIND, TAGS.VITE, TAGS.ASTRO, TAGS.NEXT, TAGS.SHADCN, TAGS.REACTQUERY, TAGS.GRAPHQL, TAGS.ApolloClientIcon ]
    },
    {
        title: 'Back End',
        tags: [TAGS.JS, TAGS.TYPESCRIPT, TAGS.HTML, TAGS.CSS, ]
    },
]

---


{
    SKILLS.map(skill => (
        <SkillItem {...skill}/>
    ))
}